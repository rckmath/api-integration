    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "mongodb": "^4.1.0",
    "mongoose": "^5.13.7",
